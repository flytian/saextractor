<?xml version="1.0" ?>
<!-- $Id: pat.xml $ A test file for different types of patterns. -->
<model name="test_pat">
<base url="."/>
<class id="TestClass" prune="0.05">

  <attribute id="a0" type="name" card="1" eng="1" enabled="0">
    <value>
      <pattern id="p1" cover="1" p="0.1" log="1">
        <lab name="P|DIV"/>
      </pattern>
      <pattern id="p2" cover="1" p="0.4" log="0" ignore="case" enabled="0">
        <lab name="^P|DIV"/> <tok/>{0,20} (privacy|policy|<tok ignore="case">sponsor</tok>) <tok/>{0,20} <lab name="P|DIV$"/>
      </pattern>
      <length> <distribution min="1" max="1000"/> </length>
    </value>
  </attribute>

  <attribute id="a1" type="name" card="1" eng="1" enabled="1">
    <value>
      <pattern id="p1" cover="0.1" p="0.7" log="1" enabled="0">
        <lab name="^P"/> <tok/> (<lab neg="1" name="P$"/> <tok/>)* <lab name="P$"/>
      </pattern>
      
      <pattern id="p2" cover="0.1" p="0.7" log="1" enabled="0">
        <lab name="^P"/> <tok/> (<lab neg="1" name="P$"/> <lab neg="1" name="BR$"/> <tok/>){20,100} <lab name="P$"/>
      </pattern>
      
      <pattern id="p3" cover="0.1" p="0.7" log="1" enabled="1">
       (<lab name="^P"/> 
        <lab name="^DIV"/>  
        <lab name="^BR"/>  
        <lab name="^LI"/> 
        <lab name="^UL"/>  
        <lab name="^OL"/>) <tok/> (<lab neg="1" name="P$"/> <lab neg="1" name="DIV$"/> <lab neg="1" name="BR$"/>  <lab neg="1" name="LI$"/>  <lab neg="1" name="UL$"/> <lab neg="1" name="OL$"/> <tok/>){20,100}  (<lab 
             name="P$"/>  
        <lab name="DIV$"/>  
        <lab name="BR$"/>  
        <lab name="LI$"/> 
        <lab name="UL$"/>  
        <lab name="OL$"/>)
      </pattern>
      <length> <distribution min="1" max="1000"/> </length>
    </value>  
  </attribute>

  <attribute id="att1" type="name" card="1" eng="1" enabled="0">
    <value><pattern cover="0.3" p="1">
      A ( B C{0,14} ){0,5} D
    </pattern></value>
  </attribute>
  
  <attribute id="att2" type="name" card="1" eng="1" enabled="0">
    <pattern id="a1a">B C{2,8}</pattern>
    <pattern id="a1b">A <pattern ref="a1a"/>{2,5} D</pattern>
    <value><pattern cover="0.3" p="1">
      <pattern ref="a1b"/>{1,2}
    </pattern></value>
  </attribute>
  
  <attribute id="att3" type="name" card="1" eng="1" enabled="0">
    <value><pattern cover="0.3" p="1">
      B{3,5}
    </pattern></value>
  </attribute>

  <attribute id="att4" type="name" card="1" eng="1" enabled="0">
    <!-- A <tok/>{0,5} ( B ( C | D ) <tok/>{0,8} ) {2,5} (D | X) -->
    <value><pattern cover="0.3" p="0.9">
        A <tok/>{0,5} D
      </pattern></value>
  </attribute>
  
  <attribute id="att4" type="name" card="1" eng="1" enabled="0">
    <value><pattern cover="0.3" p="0.9">
        B (C|karel){0,3} B C{3,4}
      </pattern></value>
  </attribute>

  <attribute id="att4" type="name" card="1" eng="1" enabled="0">
    <value><pattern cover="0.3" p="0.9">
        B ((C|karel){0,3} B C{3,4}) {2,10} B
      </pattern></value>
  </attribute>

  <attribute id="att5" type="name" card="1" eng="1" enabled="0">
   <pattern id="keys">X|Y</pattern>
   <pattern id="ins"> <lab neg="1" name="s$"/><tok/> </pattern>
   <value><pattern cover="0.3" p="0.9">
     <lab name="^s"/> (<pattern ref="keys"/><pattern ref="ins"/>{0,5}){2,10} <lab name="s$"/>
     <lab name="^s"/> <tok/> <pattern ref="ins"/>{0,4} (<pattern ref="keys"/><pattern ref="ins"/>{0,5}){2,10} <lab name="s$"/>
   </pattern></value>
  </attribute>
  
  <attribute id="street" type="name" card="1" eng="1" enabled="0">
   <pattern id="cz_street" log="1" src="streets.txt" type="pattern" ignore="case" encoding="utf-8" />
   <!-- 
   <pattern id="cz_street2" log="1" type="pattern">
     MASARYKOVA
     TYRLOVA
   </pattern>
   <pattern id="cz_street3" log="1" type="pattern" src="streets.txt" />
   -->
   <value>
     <pattern p="0.8" cover="0.7"> <pattern ref="cz_street"/> </pattern>
   </value>	
  </attribute>
  
  <attribute id="otok" type="name" card="1" eng="1" enabled="0">
   <pattern id="surname1_easy">Novak|Smith|Simpson|Wayne|Kokoska</pattern>
   <pattern id="surname1">
     <pattern src="../../../data/names/cz/surnames20k.txt" type="pattern" ignore="case" case="CA|UC" encoding="utf-8" />
     <pattern src="../../../data/names/en/last18k.txt" type="pattern" ignore="case" case="CA|UC" encoding="iso-8859-1" />
   </pattern>
   <pattern id="surname">
     <pattern ref="surname1"/>
     <pattern ref="surname1"/> - (<pattern ref="surname1"/> | <tok type="alpha|apos" case="CA|UC"/>)
     (<pattern ref="surname1"/> | <tok type="alpha|apos" case="CA|UC"/>) - <pattern ref="surname1"/>
   </pattern>
   <value>
     <pattern p="0.8" cover="0.7"> <pattern ref="surname"/> </pattern>
   </value>
  </attribute>

  <attribute id="john" type="name" card="1" eng="1" enabled="0">
    <value><pattern cover="0.3" p="1" log="1">
      John Doe (,? jr)?
    </pattern></value>
  </attribute>
  

</class>
</model>
