<h1>Department for Continuing Education Maps</h1>
<section class="twocol left">
    <p>This site hosts maps to support the department's students, courses, and visitors.</p>

    <h2>Maps in course subject areas</h2>
    <ul class="nodeSubNav"><li><a href="/archaeology/index.php" class="">Archaeology</a></li><li><a href="/architecturalhistory/index.php" class="">Architectural History</a></li><li><a href="/envsci/index.php" class="">Environmental Science</a></li><li><a href="/history/index.php" class="">History</a></li><li><a href="/literature/index.php" class="">Literature</a></li></ul>

    <h2>The Department's buildings</h2>
    <p>The <a href="http://www.conted.ox.ac.uk/">Department for Continuing Education</a> is distributed between <a href="?id=rewley" class="mapLink">Rewley House</a>, and <a href="?id=ewert" class="mapLink">Ewert House</a>, in Oxford.</p>
</section>

<section class="twocol right">
    <div id="map" style="height: 30em;">Javascript is required to load maps.</div>
</section>
<div class="clear"> </div>
</div>

<script>
    $('#map').html('');

    var map = L.map('map').setView([51.77, -1.263], 12);
    L.tileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {attribution: "Map data Â© OpenStreetMap contributors"}).addTo(map);

    var buildings = {"type":"Feature Collection","features":[
        {"type":"Feature","id":"ewert","geometry":{"type":"Point","coordinates":["-1.26275","51.77665"]},"properties":{
                "name":"Ewert House", "info": '<address>Ewert Place, Summertown,<br/>Oxfordshire,<br/>OX2 7DD,<br/>United Kingdom</address>'}},
        {"type":"Feature","id":"rewley","geometry":{"type":"Point","coordinates":["-1.2622","51.75723"]},"properties":{
                "name":"Rewley House", "info": '<address>1 Wellington Square, Oxford,<br/>Oxfordshire,<br/>OX1 2JA,<br/>United Kingdom</address>'}}
    ]};

    var markers = {};
    buildingsLayer = L.geoJson(buildings, {
        pointToLayer: function (feature, latlng) {
            var marker = new L.marker(latlng, {title: feature.properties.name});
            markers[feature.id] = marker;
            return marker;
        },
        onEachFeature: function(feature, layer) {
            layer.bindPopup("<h2>"+feature.properties.name+"</h2>"+feature.properties.info);
        }
    }).addTo(map);


    $('a.mapLink').on('click', function(e) {
        e.preventDefault();
        markers[getParam($(this).attr('href'), 'id')].openPopup();
    });

    if (window.location.search) {
        markers[getParam(window.location.search, 'id')].openPopup();
    }
</script>

