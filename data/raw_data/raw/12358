		//If we're in a modal dialogue and access denied when access parent, then hide the close buttons
		function HideCloseButtons()
		{
			try{
				var test = window.frameElement;
			}
			catch (e){
				inputFields = document.getElementsByTagName('input');
				for (var i=0; i<inputFields.length;i++)
				{
					if (inputFields[i].value == 'Close')
					{
						inputFields[i].style.display='none';
					}
				}
			}
		}
	</script>
	
