states=new Array()
function hideallExcept(elm) {
 for (var i = 0; i < states.length; i++) {
 var layer;
 if (layer = document.getElementById(states[i])) {
    if (elm!= states[i]) {
      layer.style.display = "none";
     }
    else {
    layer.style.display = "block";
     }
   }
  }
}
function showall() {

for (var i = 0; i < states.length; i++) {
 var layer;
  if (layer = document.getElementById(states[i]) ) {;
       layer.style.display = "block";
     }
  }  
}
