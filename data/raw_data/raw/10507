<script type="text/javascript">
function fade1(div_id,button){
	if(button.value=='FadeOut'){
		$('#'+div_id).fadeOut('slow');
		button.value='FadeIn';
	} else
	{
		$('#'+div_id).fadeIn('slow');
		button.value='FadeOut';
	}
}

function fade2(div_id){
	$('#'+div_id).fadeOut('slow');
}

function fade3(div_id){
	$('#'+div_id).fadeIn('slow');
}
</script>
