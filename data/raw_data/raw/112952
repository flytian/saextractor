<script type='text/javascript' src='/wp-content/plugins/ltw-content-shortcodes/js/functions.js?ver=1.0.3'></script>
<script type='text/javascript' src='/wp-content/plugins/twitget/js/moment.js?ver=4.1.5'></script>
<script type='text/javascript' src='/wp-content/plugins/twitget/js/langs.min.js?ver=4.1.5'></script>
<script type="text/javascript" charset="utf-8">
jQuery(function($){			
	var isEmbed = window != window.parent; 
	if(!isEmbed){
		$("div.tribe-events-category-holywell-music-room-events").remove(); }
	else{
		$("header#header, footer#footer, #wpadminbar, .filter-categories").remove();
		$(".hollywell-calendar").css({'overflow':'hidden'});
		$(".tribe-events-category-holywell-music-room-events").css({'display':'block'});
	}
});
$(window).load(function() {	
		$( ".hollywell-calendar" ).contents().find( "div.hentry" ).css( "display", "none" );
		$( ".hollywell-calendar" ).contents().find( "div.tribe-events-category-holywell-music-room-events" ).css( "display", "block" );
		$( ".hollywell-calendar" ).contents().find( "div#main" ).css( "padding", "0" ).css( "margin", "0" );
		$( ".hollywell-calendar" ).contents().find( ".clear.te-c" ).css( "display", "none" );
		$( ".hollywell-calendar" ).contents().find( "#display-menu" ).css( "display", "none" );
		$( ".hollywell-calendar" ).contents().find( "#tribe-bar-views" ).remove();
		$("iframe.hollywell-calendar").show();		
});
jQuery(function($) {	
  $(document).ajaxStop(function()
  {
    // Executed when all ajax requests are done.	 
	$("div.tribe-events-category-holywell-music-room-events").hide();
	window.parent.$( "#hollywell-cal" ).contents().find( "div.hentry" ).hide();
	window.parent.$( "#hollywell-cal" ).contents().find( "div.tribe-events-category-holywell-music-room-events" ).show();	
  });
});
</script> 
