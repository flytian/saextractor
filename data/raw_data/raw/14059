    nodeList[ 0 ] = 'node1';
    nodeList[ 1 ] = 'node2';
    nodeList[ 2 ] = 'node3';
    nodeList[ 3 ] = 'node4';
    nodeList[ 4 ] = 'node5';
    nodeList[ 5 ] = 'node6';
    nodeList[ 6 ] = 'node7';
    nodeList[ 7 ] = 'node8';
