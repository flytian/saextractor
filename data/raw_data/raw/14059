    nodeList[ 23 ] = 'node24';
    nodeList[ 24 ] = 'node25';
    nodeList[ 25 ] = 'node26';
    nodeList[ 26 ] = 'node27';
    nodeList[ 27 ] = 'node28';
    nodeList[ 28 ] = 'node29';
    nodeList[ 29 ] = 'node30';
    nodeList[ 30 ] = 'node31';
    nodeList[ 31 ] = 'node32';
