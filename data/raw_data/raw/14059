  <script type="text/javascript">
  function js_go_to_row( rowNumber ) {

    var foundStart = false;

    rowNumber = parseInt( rowNumber );
    if( isNaN( rowNumber )) {
      alert( 'Invalid row number.' );
      return;
    }

    var search = window.location.search;
    if( search.length == 0 ) {
      window.location.search = '?start=' + rowNumber;
      return;
    }

    var parts = search.split( '&' );
    var numParts = parts.length;

    for( i = 0; i < numParts; i++ ) {
      var part = parts[ i ];
      if( i == 0 ) {
        var firstChar = part.substring( 0, 1 );
        if( firstChar == '?' ) {       // that's what it *should* be!
          part = part.substring( 1 );  // trim off the leading question mark
          parts[i] = part;
        }
      }

      var pair = part.split( '=' );
      if( pair.length == 2 ) { // once again, that's what it *should* be!
        var searchName = pair[0];
        if( searchName == 'start' ) {
          foundStart = true;
          parts[i] = searchName + '=' + rowNumber;
          break;
        }
      }
      else {
        alert( 'Invalid search term(s).' );
        return;
      }
    }

    var newSearch = '?' + parts.join( '&' );

    if( ! foundStart ) {
      newSearch = newSearch + '&start=' + rowNumber;
    }
    window.location.search = newSearch;
  }
  </script>



  


  <div class="pagination">

      



<button class="button tiny" onclick="var current_search=window.location.search; window.location.href='/print_results' + current_search">
打印 | print
</button>


  </div><!--class:pagination-->


