  <script>
  function expandOneTreeNode ( div_id, button_id ) {
    var the_div = document.getElementById( div_id );
    var the_button = document.getElementById( button_id );
    the_div.style.display = "";
    the_button.innerHTML = "-";

    the_div = document.getElementById( "treenav_buttons_bottom" );
    the_div.style.display = "";
  }
  </script>

  
  
  

  <script>
  function collapseOneTreeNode ( div_id, button_id ) {
    var the_div = document.getElementById( div_id );
    var the_button = document.getElementById( button_id );
    the_div.style.display = "none";
    the_button.innerHTML = "+";
  }
  </script>

  

  
  

  <script>
  function toggleNodeDisplay( div_id, the_button ) {
    var the_div = document.getElementById( div_id );
    var current_setting = the_div.style.display;

    if( current_setting == "none" ) {
      the_div.style.display = "";
      the_button.innerHTML = "-";

      the_div = document.getElementById( "treenav_buttons_bottom" );
      the_div.style.display = "";
    }
    else {
      the_div.style.display = "none";
      the_button.innerHTML = "+";
    }
  }
  </script>

  
  
  
  
  

