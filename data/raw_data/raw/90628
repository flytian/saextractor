	function urlParams() {
		var e,
		    u = {},
		    a = /\+/g,
		    r = /([^&=]+)=?([^&]*)/g,
		    d = function (s) { return decodeURIComponent(s.replace(a, " ")); },
		    q = window.location.search.substring(1);

		while (e = r.exec(q))
			u[d(e[1])] = d(e[2]);

		return u;
	};

	jQuery(document).ready(function() {
		var params = urlParams();
		//console.log("q is " + params["q"]);
		if (params["q"] && params["q"].slice(0,3) == 'chw')
		{
			//console.log(" first 3 chars are: " + params["q"].slice(0,3));
			Z.showImage("zoom",params["q"],"zSkinPath=Assets/Skins/Light&zNavigatorVisible=0&zToolbarVisible=1&zToolbarPosition=0&zLogoVisible=0&zInitialX=2800&zInitialY=3300&zInitialZoom=10");
		}

		if (!params["q"])
		{
			jQuery(".first").css('background-color', '#FFFFFF');
		}

	});

</script>


