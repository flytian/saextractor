<!--Altmetric.com integration with DOI v1.2-->

<script type="text/javascript">
$("h3.EXLResultFourthLine").each(function(){
        // Find any valid DOI style number in the forth line and then store as DOI and use for Almetric badge
        if($(this).text().match(/\b(10[.][0-9]{4,}(?:[.][0-9]+)*(?:(?!["&\'<>])\S)+)\b/g)){
                        var DOI=$(this).text().match(/\b(10[.][0-9]{4,}(?:[.][0-9]+)*(?:(?!["&\'<>])\S)+)\b/g);
                        // Additional check for reading restrictions
                        if($(this).text().match(/\*\*\*/g)){
                    $(this).html('<b>*** This copy is available via Bodleian Libraries reading room PCs only; <a href="http://ox.libguides.com/content.php?pid=228044&amp;sid=3652852#13124553" target="_blank">restrictions apply</a> ***</b><br><br><div data-badge-popover="right" data-badge-type="2" data-hide-no-mentions="true" data-link-target="_blank" class="altmetric-embed" data-doi="'+DOI[0]+'"></div>');
                        } else {
                            $(this).html('<br><div data-badge-popover="right" data-badge-type="2" data-hide-no-mentions="true" data-link-target="_blank" class="altmetric-embed" data-doi="'+DOI[0]+'"></div>');
                        }
        // Find reference for PMID and then pull 8 digit number
        } else if ($(this).html().match(/pmid/i)&&$(this).html().match(/\d{8}/)){
                        var PMID=$(this).html().match(/\d{8}/);
                        $(this).html('<br><div data-badge-popover="right" data-badge-type="2" data-hide-no-mentions="true" data-link-target="_blank" class="altmetric-embed" data-pmid="'+PMID[0]+'"></div>');
        } else {
                //Check for ISBN,ISSN,UUID,URN then just return everything before and finished with html h3 tag.
                if($(this).html().match(/(.*?)(?=(\| )?(<b>)?isbn:|(\| )?(<b>)?isbn13:|(\| )?(<b>)?issn:|(\| )?(<b>)?e-issn:|(\| )?(<b>)?uuid:|(\| )?(<b>)?urn:|(\| )?(<b>)?(<span class="searchword">)?lsidy)/i)){
                        var new_forth_line=$(this).html().match(/(.*?)(?=(\| )?(<b>)?isbn:|(\| )?(<b>)?isbn13:|(\| )?(<b>)?issn:|(\| )?(<b>)?e-issn:|(\| )?(<b>)?uuid:|(\| )?(<b>)?urn:|(\| )?(<b>)?(<span class="searchword">)?lsidy)/i);
                        //Additional statement added to remove extra delimiter | character for electronic resources
                        new_forth_line[0]=new_forth_line[0].replace(/\| <br><b>\*\*\*/g,"<br><b>***");
                        $(this).html(new_forth_line[0]+"</h3>");
                };
                //Another exception for digitized copy
                var new_forth_line=$(this).html().replace(/\| <br><b>\*\*\*/g,"<br><b>***");
                $(this).html(new_forth_line);
        };
});
</script>

