<script>
var numOfTweetsToShow = 20;
jQuery(function($){
  $(".footerTweet").tweet({
  	modpath: '/twitter/',
    join_text: "auto",
    username: "LincolnAlumni",
    count: numOfTweetsToShow,
    loading_text: "loading tweets...",
    template: "{text}... {time}"
  });

  // twitter navigation functionality
  // disable right click for tweet nav
  $('.footer .tweetBar .tweetNav').live('contextmenu', function(e){
	return false;
  });
  // disable drag selection for tweetBar
  $('.footer .tweetBar').attr('unselectable','on').css({
	'-moz-user-select':'-moz-none',
	'-moz-user-select':'none',
	'-o-user-select':'none',
	'-khtml-user-select':'none', /* you could also put this in a class */
	'-webkit-user-select':'none',/* and add the CSS class here instead */
	'-ms-user-select':'none',
	'user-select':'none'
  }).bind('selectstart', function(){
  	return false;
  });

  var currentTweet = '0';
  $('.tweet_list li:gt('+currentTweet+')').hide();

  // disable the nav buttons if there is nothing to show
  var tweetPrevOpacity;
  var tweetNextOpacity;
  function checkTweetVal(){
  	// prev button
	if (parseFloat(currentTweet)<=0){
		$('#tweetPrev').css('opacity', '0.4');
		$('#tweetPrev').css('cursor', 'context-menu');

	} else {
		$('#tweetPrev').css('opacity', '1.0');
		$('#tweetPrev').css('cursor', 'pointer');
	}
	// next button
	if (parseFloat(currentTweet)>=parseFloat(numOfTweetsToShow)-1){
		$('#tweetNext').css('opacity', '0.4');
		$('#tweetNext').css('cursor', 'context-menu');
	} else {
		$('#tweetNext').css('opacity', '1.0');
		$('#tweetNext:hover').css('cursor', 'pointer');
	}
	tweetPrevOpacity = $('#tweetPrev').css('opacity');
	tweetNextOpacity = $('#tweetNext').css('opacity');
	//console.log('prev - '+tweetPrevOpacity+',  next - '+tweetNextOpacity);
  }
  checkTweetVal();
 
  $('#tweetPrev').click(function(){
  	checkTweetVal();
  	if (tweetPrevOpacity === '1'){
  		if (!$('.tweet_list li:animated').length>0){
		  	var prevTweet = parseFloat(currentTweet)-1;
		  	$('.tweet_list li:eq('+currentTweet+')').fadeOut('slow',function(){
		  		$('.tweet_list li:eq('+prevTweet+')').fadeIn();
	  		});
	  		currentTweet = prevTweet;
	  		//console.log('previous tweet.  currently showing tweet is '+currentTweet);
	  		checkTweetVal();
	  	}
	} else {
		//console.log('negative value so no going back');
	}
  });

  $('#tweetNext').click(function(){
  	checkTweetVal();
  	if (tweetNextOpacity === '1'){
  		if (!$('.tweet_list li:animated').length>0){
		  	var nextTweet = parseFloat(currentTweet)+1;
		  	$('.tweet_list li:eq('+currentTweet+')').fadeOut('slow',function(){
		  		$('.tweet_list li:eq('+nextTweet+')').fadeIn();
		  	});
		  	currentTweet = nextTweet;
		  	//console.log('next tweet.  currently showing tweet is '+currentTweet);
		  	checkTweetVal();
		}
	} else {
		//console.log('max tweet limit reached, disable click functionality');
	}
  });
});

</script>
