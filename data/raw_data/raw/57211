//<![CDATA[
function loadSearchDbPage(){
	var fn;
	addLightBoxDivs();
	fn='loadSearchDbPage';
	var timestamp = new Date().getTime();
	var url = "searchDB.do"+"?fn="+fn+"&ts="+timestamp;
	var data;
	$.ajax({
	    url:url,
		data:data,
		global:false,
