<script type="text/javascript">
// <![CDATA[


var pageTitle=document.title;

if(pageTitle)
{
	if(encodeURIComponent){pageTitle=encodeURIComponent(pageTitle);}
	else
	if(encodeURLComponent){pageTitle=encodeURLComponent(pageTitle);}
	else
	if(escape){pageTitle=escape(pageTitle);}
	
	
	var anchorIds=["facebook","twitter","pinterest"];
	for(var i=0;i<anchorIds.length;i++)
	{
		var anchor=HtmlUpdater.$(anchorIds[i]);
		if(!anchor){continue;}
		if(!anchor.href){continue;}
		
		var href=anchor.href;
		
		var eq=href.lastIndexOf("=");
		var at=href.indexOf('@');
		
		if(at==-1)
		{
			anchor.href=href.substring(0,eq+1)+pageTitle+"&";
		}
		else
		{
			anchor.href=href.substring(0,eq+1)+document.title+" "+href.substring(at);
		}
	}

}

// ]]>
</script>
<script type="text/javascript">
// <![CDATA[
var today=new Date();
today.setHours(0);
var tomorrow=new Date();
tomorrow.setHours(23);
tomorrow.setDate(today.getDate()+1);

loadICalAsNews("/feeds/SeminarSeries-All.ics?","strachey",null,null,{heading:"Strachey lectures",filterFunction:function(event){if(event.CATEGORIES!='strachey'){return false;}else{return true;}}});
loadICalAsNews("/feeds/SeminarSeries-All.ics?","industry",null,null,{heading:"Industry seminars",filterFunction:function(event){if(event.CATEGORIES!='industry'){return false;}else{return true;}}});
loadICalAsNews("/feeds/SeminarSeries-All.ics?","recentEvents",null,null,{heading:"All Seminars and lectures"});

loadICalAsNews("/feeds/Timetable-Lecture.ics?","lectures",today,tomorrow);
loadICalAsNews("/feeds/Timetable-Practical.ics?","practicals",today,tomorrow);

// ]]>>

</script>


