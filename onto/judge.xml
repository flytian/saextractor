<?xml version="1.0" encoding="UTF-8"?>
<!--Judge whether a page is seminar page-->
<!DOCTYPE judge SYSTEM "http://static.ileodo.com/s/mscproject/judge.dtd">
<judge policy="sum">
    <ruleset sel="title" on="text" policy="max">
        <reg weight="10" exp="seminar" ingnoreCase="true" min="1" max="2"/>
    </ruleset>
    <ruleset sel="body" on="text" policy="sum">
        <reg weight="10" exp="seminar" min="1" max="2"/>
        <reg weight="10" exp="(location)" min="1" max="2"/>
        <reg weight="10" exp="(speaker)" min="5"/>
        <reg weight="" exp="\b(date)?\b" min=""/>
        <reg weight="" exp="(time)"/>
    </ruleset>
</judge>