<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--Judge whether a page is seminar page-->
<!DOCTYPE featurespace PUBLIC "Seminar"
        "http://static.ileodo.com/s/mscproject/featurespace.dtd">
<featurespace name="seminarpages" version="1.0">
    <feature id="f1"
             name="E:url[seminar]"
             description="the word `seminar(s)` exist in url">
        <list sel="url" src="seminar.wd" policy="sum"/>
    </feature>

    <feature id="f2"
             name="#:text[abstract]"
             description="number of word `abstract` (synonym) in TEXT">
        <list src="abstract.wd" policy="sum"/>
    </feature>

    <feature id="f3"
             name="#:text[location]"
             description="number of word `location` (synonym) in TEXT">
        <list src="location.wd" policy="sum"/>
    </feature>

    <feature id="f4"
             name="#:text[LOCATION]"
             description="number of location entities in TEXT">
        <list src="location.ent" policy="sum"/>
    </feature>

    <feature id="f5"
             name="#:text[speaker]"
             description="number of word `speaker` (synonym) in TEXT">
        <list src="speaker.wd" policy="sum"/>
    </feature>

    <feature id="f6"
             name="#:text[SPEAKER]"
             description="number of speaker entities in TEXT">
        <list src="speaker.ent" policy="sum"/>
    </feature>

    <feature id="f7"
             name="#:text[date|time]"
             description="number of word `date|time` (synonym) in TEXT">
        <list src="datetime.wd" policy="sum"/>
    </feature>

    <feature id="f8"
             name="#:text[DATE|TIME]"
             description="number of DATE and TIME entities in TEXT">
        <list src="datetime.ent" policy="sum"/>
    </feature>

    <feature id="f9"
             name="#:tag[img]"
             description="number of image in page">
        <reg sel="tag" exp="<img .*?>"/>
    </feature>
    <!-- already labeled with same hash, url, same content_hash-->

</featurespace>